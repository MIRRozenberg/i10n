#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:40
msgid "Flatpaks"
msgstr "Flatpaks"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:43
msgid "Install"
msgstr "Gosod"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:52
msgid "Remove"
msgstr "Tynnu"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:61
msgid "Upgrade"
msgstr "Diweddaru"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:69
msgid "Please take a look at the list of changes below."
msgstr "Edrychwch ar y rhestr newidiadau isod"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:79
msgid "Additional software has to be installed"
msgstr "Rhaid gosod meddalwedd ychwanegol"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:81
msgid "Additional software has to be removed"
msgstr "Rhaid tynnu meddalwedd ychwanegol"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:83
msgid "Additional software has to be upgraded"
msgstr "Rhaid diweddaru meddalwedd ychwanegol"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:92
msgid "Additional changes are required"
msgstr "Mae angen newidiadau ychwanegol"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:98
#, python-format
msgid "%s will be downloaded in total."
msgstr "Bydd %s yn cael ei lwytho i lawr i gyd"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:102
#, python-format
msgid "%s of disk space will be freed."
msgstr "Bydd %s o le ar ddisg yn cael ei ryddhau."

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:106
#, python-format
msgid "%s more disk space will be used."
msgstr "Bydd %s mwy o le ar ddisg yn cael ei ddefnyddio."

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:180
msgid "Flatpak Progress"
msgstr "Cynnydd Flatpak"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:226
msgid "Cancelled"
msgstr "Diddymwyd"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:227
msgid "The operation was cancelled before it could complete."
msgstr "Cafodd y weithred ei diddymu cyn ei chwblhau."

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:229
msgid "Initializing"
msgstr "Cychwyn"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:232
msgid "Installing flatpaks"
msgstr "Gosod Flatpaks"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:233
#, python-format
msgid "Installing: %s"
msgstr "Gosod: %s"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:235
msgid "Removing flatpaks"
msgstr "Tynnu Flatpaks"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:236
#, python-format
msgid "Removing: %s"
msgstr "Tynnu: %s"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:238
msgid "Updating flatpaks"
msgstr "Diweddaru Flatpaks"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:239
#, python-format
msgid "Updating: %s"
msgstr "Diweddaru: %s"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:241
msgid "Finished"
msgstr "Wedi gorffen"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:242
msgid "Operation completed successfully"
msgstr "Cwblhawyd y weithred yn llwyddiannus"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:244
msgid "Failed"
msgstr "Wedi methu"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:245
#, python-format
msgid ""
"An error occurred:\n"
"\n"
"%s"
msgstr ""
"Digwyddodd gwall:\n"
"\n"
"%s"

#: usr/lib/python3/dist-packages/mintcommon/installer/dialogs.py:318
msgid "An error occurred"
msgstr "Digwyddodd gwall"

#: usr/lib/linuxmint/common/mint-remove-application.py:33
msgid ""
"This menu item is not associated to any package. Do you want to remove it "
"from the menu anyway?"
msgstr ""
"Nid yw'r eitem dewislen yma wedi ei gysylltu ag unrhyw becyn. Hoffech chi ei "
"dynnu o'r ddewislen beth bynnag?"

#: usr/lib/linuxmint/common/mint-remove-application.py:74
msgid "The following packages will be removed:"
msgstr "Bydd y pecynnau canlynol yn cael eu tynnu:"

#: usr/lib/linuxmint/common/mint-remove-application.py:81
msgid "Packages to be removed"
msgstr "Pecynnau i'w tynnu"

#: usr/lib/linuxmint/common/mint-remove-application.py:142
#, python-format
msgid "Cannot remove package %s as it is required by other packages."
msgstr "Methu tynnu pecyn %s am fod ei angen ar becynnau eraill."

#: usr/lib/linuxmint/common/mint-remove-application.py:148
#, python-format
msgid "Package %s is a dependency of the following packages:"
msgstr "Mae'r pecynnau canlynol yn dibynnu ar becyn %s :"

